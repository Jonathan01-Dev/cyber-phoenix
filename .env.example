# Port TCP par defaut (peut etre surcharge par --port)
ARCHIPEL_TCP_PORT=7777

# Discovery
ARCHIPEL_HELLO_INTERVAL=30
ARCHIPEL_PEER_TIMEOUT=90

# Keep-alive TCP
ARCHIPEL_KEEPALIVE_INTERVAL=15
ARCHIPEL_KEEPALIVE_TIMEOUT=45
ARCHIPEL_HANDSHAKE_MAX_SKEW=120

# Capacite serveur TCP
ARCHIPEL_MAX_CONNECTIONS=64

ARCHIPEL_DISCOVERY_BROADCAST_FALLBACK=true
ARCHIPEL_DISCOVERY_LOCALHOST_FALLBACK=true

# Forcer l'IP d'interface reseau a utiliser pour la decouverte
# Exemple: ARCHIPEL_INTERFACE_IP=192.168.137.119
# ARCHIPEL_INTERFACE_IP=
